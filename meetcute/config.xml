<?xml version='1.0' encoding='utf-8'?>
	<widget id="ca.edumedia.kum00003" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
		<name>Sercan MeetCute</name>
		<description>
			A sample Apache Cordova application that responds to the deviceready event.
		</description>
		<author email="dev@cordova.apache.org" href="http://cordova.io">
			Apache Cordova Team
		</author>
		<content src="index.html" />
		<plugin name="cordova-plugin-whitelist" spec="1" />
		<access origin="*" />
		<allow-intent href="http://*/*" />
		<allow-intent href="https://*/*" />
		<allow-intent href="tel:*" />
		<allow-intent href="sms:*" />
		<allow-intent href="mailto:*" />
		<allow-intent href="geo:*" />
		<platform name="android">
			<allow-intent href="market:*" />
		</platform>
		<platform name="ios">
			<splash src="platforms/ios/www/res/screens/ios/screen-ipad-portrait-2x.png" width="1536" height="2048" />
			<splash src="platforms/ios/www/res/screens/ios/screen-ipad-portrait.png" width="768" height="1024" />
			<splash src="platforms/ios/www/res/screens/ios/screen-iphone-portrait-2x.png" width="640" height="960" />
			<splash src="platforms/ios/www/res/screens/ios/screen-iphone-portrait-568h-2x.png" width="640" height="1136" />
			<splash src="platforms/ios/www/res/screens/ios/screen-iphone-portrait-667h.png" width="750" height="1334" />
			<splash src="platforms/ios/www/res/screens/ios/screen-iphone-portrait-736h.png" width="1242" height="2208" />
			<splash src="platforms/ios/www/res/screens/ios/screen-iphone-portrait.png" width="320" height="480" />
			<!-- iOS 8.0+ -->
			<!-- iPhone 6 Plus  -->
			<icon src="platforms/ios/www/res/icons/ios/icon-60-3x.png" width="180" height="180" />
			<!-- iOS 7.0+ -->
			<!-- iPhone / iPod Touch  -->
			<icon src="platforms/ios/www/res/icons/ios/icon-60.png" width="60" height="60" />
			<icon src="platforms/ios/www/res/icons/ios/icon-60-2x.png" width="120" height="120" />
			<!-- iPad -->
			<icon src="platforms/ios/www/res/icons/ios/icon-76.png" width="76" height="76" />
			<icon src="platforms/ios/www/res/icons/ios/icon-76-2x.png" width="152" height="152" />
			<!-- iOS 6.1 -->
			<!-- Spotlight Icon -->
			<icon src="platforms/ios/www/res/icons/ios/icon-40.png" width="40" height="40" />
			<icon src="platforms/ios/www/res/icons/ios/icon-40-2x.png" width="80" height="80" />
			<!-- iPhone / iPod Touch -->
			<icon src="platforms/ios/www/res/icons/ios/icon.png" width="57" height="57" />
			<icon src="platforms/ios/www/res/icons/ios/icon-2x.png" width="114" height="114" />
			<!-- iPad -->
			<icon src="platforms/ios/www/res/icons/ios/icon-72.png" width="72" height="72" />
			<icon src="platforms/ios/www/res/icons/ios/icon-72-2x.png" width="144" height="144" />
			<!-- iPhone Spotlight and Settings Icon -->
			<icon src="platforms/ios/www/res/icons/ios/icon-small.png" width="29" height="29" />
			<icon src="platforms/ios/www/res/icons/ios/icon-small-2x.png" width="58" height="58" />
			<!-- iPad Spotlight and Settings Icon -->
			<icon src="platforms/ios/www/res/icons/ios/icon-50.png" width="50" height="50" />
			<icon src="platforms/ios/www/res/icons/ios/icon-50-2x.png" width="100" height="100" />
			<allow-intent href="itms:*" />
			<allow-intent href="itms-apps:*" />
		</platform>
		
		
	</widget>